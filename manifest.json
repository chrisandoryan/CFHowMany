{
    "manifest_version": 3,
    "name": "CFHowMany? A Cloudflare-tracking Project",
    "version": "1.1",
    "permissions": ["webRequest", "webRequestBlocking", "storage", "tabs", "dns"],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    }
  }
  